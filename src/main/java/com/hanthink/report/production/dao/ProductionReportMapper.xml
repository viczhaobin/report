<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hanthink.report.production.dao.ProductionReportMapper">
	<select id="selectProductionSummery" resultType="java.util.Map" parameterType="Map">
		select * from etl_om_production_summery
	            where 
	                cal_type = #{cal_type}
	</select>
</mapper>